{
  "name": "project-name",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \" npm run dev:web \"",
    "dev:server": "cross-env NODE_ENV=development concurrently --handle-input \"wait-on dist/server/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "dev:web": "cross-env NODE_ENV=development node client/dev.server.js",
    "build": "concurrently \"npm run build:server\" \" npm run build:web \"",
    "build:server": "rimraf dist && tsc -p tsconfig.build.json",
    "build:web": "next build && node ./build/remove-css-link.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server/main.js",
    "lint": "eslint **/*.{ts,js,tsx,jsx} --ignore-path .eslintignore",
    "format": "prettier **/**/*.{ts,tsx,js,jsx} --config ./.prettierrc --ignore-path ./.prettierignore --write",
    "start:debug": "nodemon --config nodemon-debug.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.8.4",
    "@nestjs/core": "^6.8.4",
    "@nestjs/graphql": "^6.5.3",
    "@nestjs/platform-express": "^6.8.4",
    "apollo-server-express": "^2.9.7",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "cookie": "^0.4.0",
    "graphql": "^14.5.8",
    "graphql-tools": "^4.0.6",
    "isomorphic-unfetch": "^3.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sequelize": "^5.21.1",
    "sequelize-typescript": "^1.0.0",
    "type-graphql": "^0.17.5"
  },
  "devDependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.0.1",
    "@material-ui/core": "^4.5.1",
    "@material-ui/icons": "^4.5.1",
    "@nestjs/testing": "^6.8.4",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.19",
    "@types/node": "^12.11.7",
    "@types/sequelize": "^4.28.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "1.6.3",
    "apollo-client": "2.6.4",
    "apollo-link-context": "^1.0.8",
    "apollo-link-http": "1.5.15",
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "dayjs": "^1.8.16",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "http-proxy-middleware": "^0.20.0",
    "https-proxy-agent": "^3.0.1",
    "jest": "^24.9.0",
    "next": "^9.1.1",
    "nodemon": "^1.19.4",
    "prettier": "^1.18.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.4.0",
    "supertest": "^4.0.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.4.1",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.6.4",
    "url-loader": "^2.2.0",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
