{
  "name": "project-name",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \" npm run dev:web \"",
    "dev:server": "cross-env NODE_ENV=development nest start -p tsconfig.build.json --watch",
    "dev:web": "cross-env NODE_ENV=development node client/dev.server.js",
    "build": "concurrently \"npm run build:server\" \" npm run build:web \"",
    "build:server": "rimraf dist && tsc -p tsconfig.build.json",
    "build:web": "next build && node ./build/remove-css-link.js",
    "start:prod": "cross-env NODE_ENV=production node dist/server/main.js",
    "lint": "eslint **/*.{ts,js,tsx,jsx} --ignore-path .eslintignore",
    "format": "prettier **/**/*.{ts,tsx,js,jsx} --config ./.prettierrc --ignore-path ./.prettierignore --write",
    "start:debug": "nest start --debug --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.9.0",
    "@nestjs/core": "^6.9.0",
    "@nestjs/platform-express": "^6.9.0",
    "@types/jsonwebtoken": "^8.3.5",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "classnames": "^2.2.6",
    "cookie": "^0.4.0",
    "isomorphic-unfetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sequelize": "^5.21.2",
    "sequelize-typescript": "^1.0.0"
  },
  "devDependencies": {
    "@material-ui/core": "^4.6.1",
    "@material-ui/icons": "^4.5.1",
    "@nestjs/cli": "^6.11.5",
    "@nestjs/testing": "^6.9.0",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.7",
    "@types/sequelize": "^4.28.6",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.7.0",
    "@typescript-eslint/parser": "^2.7.0",
    "@zeit/next-css": "^1.0.1",
    "axios": "^0.19.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "dayjs": "^1.8.17",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "http-proxy-middleware": "^0.20.0",
    "https-proxy-agent": "^3.0.1",
    "jest": "^24.9.0",
    "next": "^9.1.3",
    "prettier": "^1.19.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "styled-components": "^4.4.1",
    "supertest": "^4.0.2",
    "ts-jest": "24.1.0",
    "ts-node": "8.5.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.7.2",
    "url-loader": "^2.2.0",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
